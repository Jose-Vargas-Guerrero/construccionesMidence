---
import Layout from '../layouts/Layout.astro';
---

<Layout>
	<h1>hola mundo</h1>
	<div class="box1"></div>
	<div class="box2"></div>
	<div class="box3"></div>
	<div class="box4"></div>
	<div class="box5"></div>
</Layout>

<style>
	h1{
		color: var(--dark-blue);
	}
	
	.box1{
		height: 50px;
		width: 50px;
		background-color: var(--blue);
	}
	.box2{
		height: 50px;
		width: 50px;
		background-color: var(--green);
	}
	.box3{
		height: 50px;
		width: 50px;
		background-color: var(--custom-dark);
	}
	.box4{
		height: 50px;
		width: 50px;
		background-color: var(--custom-white);
	}
	.box5{
		height: 50px;
		width: 50px;
		background-color: orange;
	}
</style>
